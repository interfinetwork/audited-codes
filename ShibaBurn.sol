    );
    
    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
    
    constructor () ERC20("ShibaBurn", "SHIBABURN", 8) {
       
        _balances[_msgSender()] = _tTotal;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        //IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
         // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;
        
        // exclude owner and this contract from fee
        _isExcludedFromFee[owner()]       = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[marketingWallet]       = true;
        _isExcludedFromFee[foundersWallet]       = true;
        _isExcludedFromFee[buybackWallet]       = true;
        //whitelist stakeholders
        _isExcludedFromFee[0x50e78787576A86e9C7Ed438C1cC9e9B23F512F9e] = true;

        _isExcludedFromFee[0x0a6868f30CBf0aE9dDC0C8f3BC00ADa93a39E1CD] = true;

        _isExcludedFromFee[0x1b10C0c7d053d6A8b0199879ee8F3a0A0F4b02A4] = true;

        _isExcludedFromFee[0x8FB8413fe19825919808069838b34ADdD799B39c] = true;

        _isExcludedFromFee[0x8a2De0A1fA6377B1f57F29030Ae2771478C0e5C5] = true;

        _isExcludedFromFee[0x2c140A83B5A71ab8d2a9eA0B3FF3ee6dc2214134] = true;

        _isExcludedFromFee[0x8aB24A7c09877243ff8D6E5A498e354Ceef841E7] = true;
        
        BuyTotalFees = _BuyliquidityFee.add(_BuymarketingFee).add(_BuyFoundersFee);
        SellTotalFees = _SellliquidityFee.add(_SellBuybackFee).add(_SellmarketingFee);
        totalFee = BuyTotalFees.add(SellTotalFees);
        _liquidityFee = _BuyliquidityFee.add(_SellliquidityFee);
        _marketingFee = _BuymarketingFee.add(_SellmarketingFee);
        _foundersFee = _BuyFoundersFee;
        _buybackFee = _SellBuybackFee;  /*
            _mint is an internal function in ERC20.sol that is only called here,
            and CANNOT be called ever again
        */
        _mint(owner(), _tTotal);
        
        launchTime = block.timestamp;
        
        
     
    }

   
    function name() public view virtual override returns (string memory) {
