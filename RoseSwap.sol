        (uint amount0, uint amount1) = IRosePair(pair).burn(to);
        (address token0,) = RoseLibrary.sortTokens(tokenA, tokenB);
        (amountA, amountB) = tokenA == token0 ? (amount0, amount1) : (amount1, amount0);
