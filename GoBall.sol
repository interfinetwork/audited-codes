contract NFTMarket is ReentrancyGuard {
    using Counters for Counters.Counter;
    Counters.Counter private _itemIds;
    Counters.Counter private _itemsSold;
    address payable owner;
    mapping(uint256 => MarketItem) private idToMarketItem;
    IERC20 public GBLToken;

    constructor(address _tokenAddress) {
        owner = payable(msg.sender);
        GBLToken = IERC20(_tokenAddress);
    }

    struct MarketItem {
        uint itemId;
        address nftContract;
        uint256 tokenId;
        address payable seller;
        address payable owner;
        uint256 salePrice;
        bool sold;
    }

    event MarketEventSync (
        string eventName,
        uint indexed idInMarket,
        address indexed nftContract,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 salePrice,
        bool sold
    );

    modifier checkIsItem(address _nftContract, uint256 _tokenId) {
        _;
    }

    modifier checkIsHero(address _nftContract, uint256 _tokenId) {
        _;
    }

    function setTokenAddress(address _tokenAddress) external {
        require(msg.sender == owner, "Permission denied!");
        GBLToken = IERC20(_tokenAddress);
    }

    function createMarketItem(address _nftContract, uint256 _tokenId, uint256 _salePrice) public payable nonReentrant {
        require(_salePrice > 0, "Require sale price > 0!");
        
        _itemIds.increment();
        uint256 idInMarket = _itemIds.current();
        idToMarketItem[idInMarket] = MarketItem (
            idInMarket,
            _nftContract,
            _tokenId,
            payable(msg.sender),
            payable(address(0)),
            _salePrice,
            false
        );

        IERC721(_nftContract).transferFrom(msg.sender, address(this), _tokenId);

        emit MarketEventSync (
            "LIST",
            idInMarket,
            _nftContract,
            _tokenId,
            payable(msg.sender),
            payable(address(0)),
            _salePrice,
            false
        );
    }

    function cancelMarketItem(address _nftContract, uint256 _idInMarket) public nonReentrant {
        require(idToMarketItem[_idInMarket].tokenId > 0, "Market Item is not exit!");
        uint256 tokenId = idToMarketItem[_idInMarket].tokenId;
        MarketItem storage currentItem = idToMarketItem[tokenId];
        require(msg.sender == currentItem.seller, "Permission denied! You are not owner of item!");

        IERC721(_nftContract).transferFrom(address(this), msg.sender, tokenId);
        delete idToMarketItem[_idInMarket];

        emit MarketEventSync (
            "DELIST",
            _idInMarket,
            _nftContract,
            tokenId,
            payable(msg.sender),
            payable(address(0)),
            0,
            false
        );
    }

    function buyItem(address _nftContract, uint256 _idInMarket, uint256 _tokenId, uint256 _amount) public checkIsItem(_nftContract, _tokenId) nonReentrant {
        require(_tokenId > 0, "TokenId is invalid!");
        require(idToMarketItem[_idInMarket].tokenId == _tokenId, "Market Item is not exit!");
        require(_amount == idToMarketItem[_idInMarket].salePrice, "Please submit the asking price in order to complete the purchase!");
        require(_nftContract == idToMarketItem[_idInMarket].nftContract, "NFTContract is invalid!");

        GBLToken.transferFrom(msg.sender, owner, _amount);
        IERC721(_nftContract).transferFrom(address(this), msg.sender, _tokenId);
        idToMarketItem[_idInMarket].owner = payable(msg.sender);

        idToMarketItem[_idInMarket].sold = true;
        _itemsSold.increment();

        emit MarketEventSync(
            "BOUGHT",
            _idInMarket,
            _nftContract,
            _tokenId,
            idToMarketItem[_idInMarket].seller,
            msg.sender,
            _amount,
            true
         );
    }

    function buyHero(address _nftContract, uint256 _idInMarket, uint256 _tokenId) public payable checkIsHero(_nftContract, _tokenId) nonReentrant {
        require(_tokenId > 0, "TokenId is invalid!");
        require(idToMarketItem[_idInMarket].tokenId == _tokenId, "Market Item is not exit!");
        require(msg.value == idToMarketItem[_idInMarket].salePrice, "Please submit the asking price in order to complete the purchase!");
        require(_nftContract == idToMarketItem[_idInMarket].nftContract, "NFTContract is invalid!");

        idToMarketItem[_idInMarket].seller.transfer(msg.value);
        IERC721(_nftContract).transferFrom(address(this), msg.sender, _tokenId);
        idToMarketItem[_idInMarket].owner = payable(msg.sender);

        idToMarketItem[_idInMarket].sold = true;
        _itemsSold.increment();
        
        emit MarketEventSync(
            "BOUGHT",
            _idInMarket,
            _nftContract,
            _tokenId,
            idToMarketItem[_idInMarket].seller,
            msg.sender,
            msg.value,
            true
         );
    }

    function getMarketItems() public view returns(MarketItem[] memory) {
        uint256 itemCount = _itemIds.current();
        uint256 unsoldItemCount = itemCount - _itemsSold.current();
        uint currentIndex = 0;

        MarketItem[] memory items = new MarketItem[](unsoldItemCount);
        for(uint i = 0; i < itemCount; i++) {
            if (idToMarketItem[i +  1].owner == address(0)) {
                uint currentId = i + 1;
                MarketItem storage currentItem = idToMarketItem[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }

        return items;
    }

    function getMyItems() public view returns(MarketItem[] memory) {
        uint256 itemCount = _itemIds.current();
        uint currentIndex = 0;
        uint myItemCount = 0;

        for(uint i = 0; i < itemCount; i++) {
            if (idToMarketItem[i + 1].owner == msg.sender) {
                myItemCount += 1;
            }
        }

        MarketItem[] memory items = new MarketItem[](myItemCount);
        for(uint i = 0; i < itemCount; i++) {
            if (idToMarketItem[i +  1].owner == address(0)) {
                uint currentId = i + 1;
                MarketItem storage currentItem = idToMarketItem[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }

        return items;
    }

    function getItemsCreatedByMe() public view returns(MarketItem[] memory) {
        uint256 itemCount = _itemIds.current();
        uint currentIndex = 0;
        uint myItemCount = 0;

        for(uint i = 0; i < itemCount; i++) {
            if (idToMarketItem[i + 1].seller == msg.sender) {
                myItemCount += 1;
            }
        }

        MarketItem[] memory items = new MarketItem[](myItemCount);
        for(uint i = 0; i < itemCount; i++) {
            if (idToMarketItem[i +  1].owner == address(0)) {
                uint currentId = i + 1;
                MarketItem storage currentItem = idToMarketItem[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }

        return items;
    }
}
